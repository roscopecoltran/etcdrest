{
  "templDir": "templates",
  "schemaURI": "file://schemas",
  "routes": [
    {
      "endpoint": "/api/v1/hosts",
      "type": "api",
      "path": "/hosts",
      "schema": "host.json"
    },
    {
      "endpoint": "/templ/{name}",
      "type": "template",
      "template": "host"
    },
    {
      "endpoint": "/schemas",
      "type": "static",
      "path": "schemas"
    }
  ]
}
